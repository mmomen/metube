<div>
  <h1><%= @playlist.name %></h1>
  <h3>Videos:</h3>
  <ol>
    <% @playlist.videos.each do |pv| %>
    <li><%= pv.title %><br>
      <iframe width="560" height="315" src="//www.youtube.com/embed/<%= pv.youtube_id %>" frameborder="0" allowfullscreen></iframe>
    </li>
    <% end %>
  </ol>
</div>